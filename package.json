{
  "name": "@cuiller/cdk-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build --filter=!./examples/* --filter=!./template/*",
    "build-storybook": "build-storybook",
    "changelog": "tsx scripts/changelog.ts",
    "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
    "changelog:format": "prettier --write .changelog",
    "changelog:latest": "tsx scripts/changelog.ts --latest",
    "changeset": "changeset",
    "clean": "pnpm -r --parallel exec pnpm dlx rimraf dist .turbo *.log node_modules && pnpm dlx rimraf dist node_modules pnpm-lock.yaml .turbo",
    "create:pkg": "plop component && pnpm install",
    "dev": "turbo run dev --filter=!./eslint/*",
    "post:dev": "pnpm run storybook",
    "prepare": "husky install",
    "reinstall": "pnpm clean && pnpm i",
    "release": "changeset publish",
    "release:dev": "changeset publish --tag dev",
    "sort-package-json": "pnpm -r --parallel exec pnpm dlx sort-package-json",
    "storybook": "start-storybook -p 7006",
    "test": "jest",
    "version": "changeset version"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "dependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@changesets/changelog-github": "0.4.7",
    "@changesets/cli": "2.25.2",
    "@changesets/get-release-plan": "3.0.15",
    "@changesets/types": "5.2.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@cuiller/styled-system": "workspace:*",
    "@octokit/rest": "^19.0.5",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.14",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "all-contributors-cli": "^6.24.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^12.0.2",
    "eslint": "8.29.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.9.1",
    "find-up": "^6.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "plop": "^3.1.1",
    "pnpm-deduplicate": "^0.4.1",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "tsup": "6.5.0",
    "tsx": "^3.12.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "pnpm": ">=7",
    "yarn": "please-use-pnpm"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}